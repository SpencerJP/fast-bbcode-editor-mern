	"scripts": {
		"start": "npm install && cd client && env-cmd -f ../.env.local react-scripts build && cd ../server && env-cmd -f ../.env.local nodemon index.js --exec babel-node --",
		"startexpress": "cd server && env-cmd -f ../.env.local nodemon index.js --exec babel-node --",
		"startreact": "cd client && env-cmd -f ../.env.local react-scripts start",
		"buildreact": "cd client && env-cmd -f ../.env.local react-scripts build",
		"test": "jest",
		"ejectreact": "cd client && react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.8.3",
		"@babel/core": "^7.8.3",
		"@babel/node": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
